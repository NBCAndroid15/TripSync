<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
       <KeyFrameSet>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:id="@+id/recyclerView4"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="10dp"
            motion:layout_constraintTop_toBottomOf="@+id/search_et_searchbar"
            motion:layout_constraintHorizontal_bias="1.0"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="10dp" />
        <Constraint
            android:id="@+id/recyclerView4"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="10dp"
            motion:layout_constraintTop_toBottomOf="@+id/search_et_searchbar"
            motion:layout_constraintHorizontal_bias="1.0"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="10dp" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_width="wrap_content"
            android:id="@+id/imageView2"
            motion:layout_constraintTop_toBottomOf="@+id/search_et_searchbar" />
        <Constraint
            motion:layout_constraintVertical_bias="0.512"
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="@+id/recyclerView4"
            motion:layout_constraintBottom_toBottomOf="@+id/recyclerView4"
            motion:layout_constraintHorizontal_bias="0.848"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:id="@+id/imageView" />
        <Constraint
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            android:id="@+id/imageView2"
            motion:layout_constraintTop_toBottomOf="@+id/search_et_searchbar"
            android:layout_marginTop="10dp" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:id="@+id/imageView2"
            android:layout_width="wrap_content"
            android:layout_height="1dp"
            motion:layout_constraintTop_toTopOf="@+id/recyclerView4"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/imageView2"
            android:layout_width="wrap_content"
            android:layout_height="1dp"
            motion:layout_constraintTop_toTopOf="@+id/recyclerView4"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/imageView"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="1dp"
            motion:layout_constraintBottom_toBottomOf="@+id/recyclerView4"
            motion:layout_constraintVertical_bias="0.386"
            motion:layout_constraintHorizontal_bias="0.791"
            motion:layout_constraintTop_toTopOf="@+id/recyclerView4"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/imageView"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="1dp"
            motion:layout_constraintBottom_toBottomOf="@+id/recyclerView4"
            motion:layout_constraintVertical_bias="0.386"
            motion:layout_constraintHorizontal_bias="0.791"
            motion:layout_constraintTop_toTopOf="@+id/recyclerView4"
            motion:layout_constraintStart_toStartOf="parent" />
    </ConstraintSet>
</MotionScene>